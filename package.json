{
  "name": "newcrossities",
  "version": "0.0.1",
  "keywords": [
    "newcrossities"
  ],
  "homepage": "https://github.com/louiechristie/newcrossities",
  "bugs": "https://github.com/louiechristie/newcrossities/issues",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "lint": "node ./node_modules/eslint/bin/eslint.js . --ext js,jsx --ignore-pattern node_modules/"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --list-different"
    ]
  },
  "dependencies": {
    "@expo/samples": "2.1.1",
    "expo": "^31.0.2",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-navigation": "^2.18.2"
  },
  "devDependencies": {
    "babel-preset-expo": "^5.0.0",
    "eslint": "^5.9.0",
    "eslint-config-universe": "^1.0.7",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.5.0",
    "husky": "^1.2.0",
    "jest-expo": "^31.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  }
}
